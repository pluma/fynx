{
  "name": "fynx",
  "version": "1.6.0",
  "description": "Formerly known as Flox. Not quite Flux.",
  "main": "index.js",
  "keywords": [
    "fynx",
    "flox",
    "flux",
    "react",
    "immutable",
    "store",
    "action",
    "axn"
  ],
  "files": [
    "lib/",
    "index.js",
    "package.json",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "lint": "jshint lib test",
    "test": "npm run lint && mocha --growl -R spec",
    "cover": "npm run lint && istanbul cover --report lcov _mocha -- -R spec",
    "coveralls": "npm run cover && cat ./coverage/lcov.info | coveralls ; rm -rf ./coverage",
    "dist": "jsxjs --es6 src lib && NODE_ENV=production webpack index.js dist/fynx.js && NODE_ENV=production COMPRESS=1 webpack index.js dist/fynx.min.js",
    "dist-watch": "dirwatch -c 'npm run dist' src"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/foss-haas/fynx.git"
  },
  "author": "Alan Plum <me@pluma.io>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/foss-haas/fynx/issues"
  },
  "homepage": "https://github.com/foss-haas/fynx",
  "devDependencies": {
    "coveralls": "^2.11.2",
    "dirwatch": "^1.1.0",
    "expect.js": "^0.3.1",
    "html-document": "^0.3.3",
    "istanbul": "^0.3.2",
    "jshint": "^2.5.6",
    "jsxjs": "^1.1.0",
    "mocha": "^2.0.1",
    "react": "^0.12.0",
    "rewire": "^2.1.0",
    "sinon": "^1.11.0",
    "webpack": "^1.5.1"
  },
  "dependencies": {
    "axn": "^1.6.0",
    "immutable": "^3.6.2"
  }
}
